<div class="uk-container  breadcrumbs">
  [[pdoCrumbs?
  &limit=`5`
  &tplWrapper=`@INLINE <ul class="uk-breadcrumb">[[+output]]</ul>`
  &tpl=`@INLINE <li><a href="[[+link]]">[[+menutitle]]</a></li>`
  &tplCurrent=`@INLINE <li class="active"><span aria-current="page">[[+menutitle]]</span></li>`
  &showHome=`1`
  &showCurrent=`1`
  ]]
</div>
